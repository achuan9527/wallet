{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev": "npx vite --mode dev -c vite.config",
    "test": "npx vite --mode test -c vite.config",
    "prd": "npx vite --mode prd -c vite.config",
    "build:dev": "NODE_OPTIONS=\"--max-old-space-size=4096\" npx --node-options=\"--max-old-space-size=4096\" --max-old-space-size=4096 vite build --mode dev -c vite.config",
    "build:test": "NODE_OPTIONS=\"--max-old-space-size=4096\" npx --node-options=\"--max-old-space-size=4096\" --max-old-space-size=4096 vite build --mode test -c vite.config",
    "build:uat": "yarn build:prd",
    "build:prd": "NODE_OPTIONS=\"--max-old-space-size=4096\" npx --node-options=\"--max-old-space-size=4096\" --max-old-space-size=4096 vite build --mode prd -c vite.config"
  },
  "dependencies": {
    "@ebay/nice-modal-react": "ebay/nice-modal-react",
    "ahooks": "^3.7.8",
    "axios": "^1.5.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "i18next": "^23.5.1",
    "lodash-es": "^4.17.21",
    "md5": "^2.3.0",
    "mitt": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "react-router-dom": "^6.16.0",
    "sass": "^1.69.5",
    "unplugin-auto-import": "^0.16.6",
    "viem": "^2.17.3",
    "vite-plugin-commonjs": "^0.9.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.0",
    "zustand": "^4.4.1",
    "zx": "^7.2.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
